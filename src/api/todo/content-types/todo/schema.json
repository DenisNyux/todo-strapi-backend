{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "singularName": "todo",
    "pluralName": "todos",
    "displayName": "Todos",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "todoTitle": {
      "type": "string",
      "required": true
    },
    "todoDescription": {
      "type": "blocks"
    },
    "todoPriority": {
      "type": "enumeration",
      "enum": [
        "critical",
        "important",
        "standart",
        "unimportant"
      ],
      "required": true,
      "default": "standart"
    },
    "todoDone": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
